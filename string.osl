////
//
// Builtin operations with strings
//
#pragma scheme-translator {
  (inline
    (define appendStrings string-append))
}
//
#pragma prolog {
  (ensure-loaded "prolog-std.scm")

  ;; Type signatures for the renamed functions
  (predicate (result-of (appendStrings . Strs) str) (all str Strs))
}
s1 + s2 = appendStrings(s1, s2)
