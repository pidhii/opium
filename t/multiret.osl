#!./install/bin/opium-osl -r

foo() = { return 1, 2, "three" }
a, b, c = foo()
assert { a::num b::num c::str }
println("a=", a, ", b=", b, ", c=", c)


bar() = values(1, 2, "three")
// assert { bar() :: (num, num, str) }
d, e, f = bar()
assert { a::num b::num c::str }
println("d=", d, ", e=", e, ", f=", f)

baz() = { return values(1, 2, 3) }
g, h, i = baz()
println(g, " ", h, " ", i)
